{
  "name": "smithandwillis",
  "type": "module",
  "private": true,
  "scripts": {
    "sb:components": "storyblok pull-components --space 332344",
    "sb:types": "npm run sb:components && storyblok-generate-ts source=./components.332344.json target=./types/storyblok",
    "dev": "npm run sb:types && nuxt dev --host",
    "dev:ssl": "local-ssl-proxy --source 3010 --target 3000 --cert localhost.pem --key localhost-key.pem",
    "build": "nuxt build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "nuke": "del-cli '.output' 'dist' '.nuxt' 'node_modules' 'package-lock.json'"
  },
  "dependencies": {
    "@ditta/colophon": "^0.0.5",
    "@storyblok/nuxt": "^6.2.4",
    "@tailwindcss/vite": "^4.0.17",
    "nuxt": "^3.16.1",
    "tailwindcss": "^4.0.17",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.11.0",
    "@michaelpumo/screen": "^0.0.18",
    "@nuxt/eslint": "^1.3.0",
    "@nuxt/image": "^1.10.0",
    "@nuxtjs/sitemap": "^7.2.9",
    "@total-typescript/ts-reset": "^0.6.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.23.0",
    "sharp": "^0.33.5",
    "storyblok-generate-ts": "^2.2.0",
    "typescript": "^5.8.2",
    "vite-svg-loader": "^5.1.0"
  }
}
